<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:element name="animales">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="tipos">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="perros">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="perro" maxOccurs="unbounded">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="raza" type="xs:string"/>
                          <xs:element name="peso" type="xs:string"/>
                          <xs:element name="color" type="xs:string"/>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="gatos">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="gato" maxOccurs="unbounded">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="raza" type="xs:string"/>
                          <xs:element name="peso" type="xs:string"/>
                          <xs:element name="color" type="xs:string"/>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
</xs:schema>




